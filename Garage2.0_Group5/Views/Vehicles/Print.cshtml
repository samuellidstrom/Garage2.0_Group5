@model Vehicle

@{
    ViewData["Title"] = "Vehicle Receipt";
}

<h1>Vehicle Receipt</h1>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LicenseNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LicenseNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TimeOfRegistration)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TimeOfRegistration)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Brand)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Brand)
        </dd>
        <vc:receipt vehicle-id="@Model.Id" />
    </dl>
</div>
